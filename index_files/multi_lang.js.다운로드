	text:			function(key){
						if( this[key] != undefined ){
							var sValue = (this[key][lang] != undefined) ? this[key][lang] : this[key].en;
							if(arguments[1]) sValue = sValue.replace(/\?/, arguments[1]);
							return sValue;
						}
						else return '';
					},

	alert:			function(sKey, sReplace){
						var obj = eval("this."+sKey);
						var sMsg = obj[lang] != undefined ? obj[lang] : obj.en;
						alert( (sReplace ? sMsg.replace(/\?/, sReplace) : sMsg) + (arguments[2] ? arguments[2] : '') );
					}
};
